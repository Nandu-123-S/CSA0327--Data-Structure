#include <stdio.h>

int main() {
    int a[100], n, key, i;

    scanf("%d", &n);              // number of elements
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);       // array input

    scanf("%d", &key);            // number to search

    for(i = 0; i < n; i++) {
        if(a[i] == key) {
            printf("Found at position %d", i+1);
            return 0;
        }
    }

    printf("Not Found");
    return 0;
}
