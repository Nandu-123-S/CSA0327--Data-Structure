#include <stdio.h>

int main() {
    int a[100], n, key, low = 0, high, mid, i;

    scanf("%d", &n);                  // number of elements
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);           // sorted array

    scanf("%d", &key);                // element to search
    high = n - 1;

    while(low <= high) {
        mid = (low + high) / 2;
        if(a[mid] == key) {
            printf("Found at position %d", mid + 1);
            return 0;
        }
        else if(key < a[mid])
            high = mid - 1;
        else
            low = mid + 1;
    }

    printf("Not Found");
    return 0;
}
