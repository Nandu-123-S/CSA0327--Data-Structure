#include <stdio.h>
#include <stdlib.h>

struct node { int data; struct node *next; };
struct node *head = NULL;

void insert(int x) {
    struct node *t = malloc(sizeof(struct node));
    t->data = x;
    t->next = head;
    head = t;
}

void display() {
    struct node *t = head;
    while(t) { printf("%d ", t->data); t = t->next; }
}

int main() {
    insert(10);
    insert(20);
    insert(30);
    display();
}
